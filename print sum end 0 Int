import java.util.Scanner;

class Main {
  static void printInt(int a) {
    System.out.println(a);
  }
  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    int total = 0;
    System.out.println("Enter your first number, input 0 to end");
    String mom = scan.nextLine();
    int a = Integer.parseInt(mom);
    total += a;
    while (a != 0){
      System.out.println("Enter your next number, input 0 to end");
      mom = scan.nextLine();
      a = Integer.parseInt(mom);
      total += a;
    }
    printInt(total);
  }
}