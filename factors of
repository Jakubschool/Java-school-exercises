import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    int total = 0;
    int number = scan.nextInt();
    for (int i=1; i<number+1; i++){
      if (number%i ==0){
        total +=1;
      }
    }
    System.out.println(total);
  }
}